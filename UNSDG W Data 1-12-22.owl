    <!-- http://www.semanticweb.org/mdebe/ontologies/UN_SDG#13c70e6a-ff8c-42c1-8784-4890f9d9d77a -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/mdebe/ontologies/UN_SDG#13c70e6a-ff8c-42c1-8784-4890f9d9d77a">
