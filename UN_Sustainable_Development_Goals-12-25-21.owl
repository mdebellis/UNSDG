    <!-- http://www.semanticweb.org/mdebe/ontologies/UN_SDG#description -->

    <owl:AnnotationProperty about="http://www.semanticweb.org/mdebe/ontologies/UN_SDG#description">
        <ns1:description>A property for the textual description of the entity. Originally I was using the rdfs:comment property but I thought it made more sense to have a separate property and leave comments for developer comments. Still needs to be an annotation property since it can apply to classes and properties as well as individuals.</ns1:description>
    </owl:AnnotationProperty>
    


    <!-- http://www.semanticweb.org/mdebe/ontologies/UN_SDG#index -->

    <owl:AnnotationProperty about="http://www.semanticweb.org/mdebe/ontologies/UN_SDG#index">
        <ns1:description>Number used to identify goals and other measures</ns1:description>
    </owl:AnnotationProperty>
    


