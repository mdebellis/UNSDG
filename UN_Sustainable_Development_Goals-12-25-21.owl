    <!-- http://www.semanticweb.org/mdebe/ontologies/UN_SDG#NoPovertyIndicator1-1-1 -->

    <owl:NamedIndividual about="http://www.semanticweb.org/mdebe/ontologies/UN_SDG#NoPovertyIndicator1-1-1">
        <type resource="http://www.semanticweb.org/mdebe/ontologies/UN_SDG#Indicator"/>
        <ns1:description>Proportion of population below the international poverty line, by sex, age, employment status and geographical location (urban/rural)</ns1:description>
        <ns1:index>1.1.1</ns1:index>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/mdebe/ontologies/UN_SDG#NoPovertyIndicator1-2-1 -->

    <owl:NamedIndividual about="http://www.semanticweb.org/mdebe/ontologies/UN_SDG#NoPovertyIndicator1-2-1">
        <type resource="http://www.semanticweb.org/mdebe/ontologies/UN_SDG#Indicator"/>
        <ns1:description>Proportion of population living below the national poverty line, by sex and age</ns1:description>
        <ns1:index>1.2.1</ns1:index>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/mdebe/ontologies/UN_SDG#NoPovertyIndicator1-2-2 -->

    <owl:NamedIndividual about="http://www.semanticweb.org/mdebe/ontologies/UN_SDG#NoPovertyIndicator1-2-2">
        <type resource="http://www.semanticweb.org/mdebe/ontologies/UN_SDG#Indicator"/>
        <ns1:description>Proportion of men, women and children of all ages living in poverty in all its dimensions according to national definitions</ns1:description>
        <ns1:index>1.2.2</ns1:index>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/mdebe/ontologies/UN_SDG#NoPovertyIndicator1-3-1 -->

    <owl:NamedIndividual about="http://www.semanticweb.org/mdebe/ontologies/UN_SDG#NoPovertyIndicator1-3-1">
        <type resource="http://www.semanticweb.org/mdebe/ontologies/UN_SDG#Indicator"/>
        <ns1:description>Proportion of population covered by social protection floors/systems, by sex, distinguishing children, unemployed persons, older persons, persons with disabilities, pregnant women, newborns, work-injury victims and the poor and the vulnerable</ns1:description>
        <ns1:index>1.3.1</ns1:index>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/mdebe/ontologies/UN_SDG#NoPovertyIndicator1-4-1 -->

    <owl:NamedIndividual about="http://www.semanticweb.org/mdebe/ontologies/UN_SDG#NoPovertyIndicator1-4-1">
        <type resource="http://www.semanticweb.org/mdebe/ontologies/UN_SDG#Indicator"/>
        <ns1:description>Proportion of population living in households with access to basic services</ns1:description>
        <ns1:index>1.4.1</ns1:index>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/mdebe/ontologies/UN_SDG#NoPovertyIndicator1-4-2 -->

    <owl:NamedIndividual about="http://www.semanticweb.org/mdebe/ontologies/UN_SDG#NoPovertyIndicator1-4-2">
        <type resource="http://www.semanticweb.org/mdebe/ontologies/UN_SDG#Indicator"/>
        <ns1:description>Proportion of total adult population with secure tenure rights to land, with legally recognized documentation and who perceive their rights to land as secure, by sex and by type of tenure</ns1:description>
        <ns1:index>1.4.2</ns1:index>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/mdebe/ontologies/UN_SDG#NoPovertyIndicator1-5-1 -->

    <owl:NamedIndividual about="http://www.semanticweb.org/mdebe/ontologies/UN_SDG#NoPovertyIndicator1-5-1">
        <type resource="http://www.semanticweb.org/mdebe/ontologies/UN_SDG#Indicator"/>
        <ns1:description>Number of deaths, missing persons and persons affected by disaster per 100,000 people</ns1:description>
        <ns1:index>1.5.1</ns1:index>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/mdebe/ontologies/UN_SDG#NoPovertyIndicator1-5-2 -->

    <owl:NamedIndividual about="http://www.semanticweb.org/mdebe/ontologies/UN_SDG#NoPovertyIndicator1-5-2">
        <type resource="http://www.semanticweb.org/mdebe/ontologies/UN_SDG#Indicator"/>
        <ns1:description>Direct disaster economic loss in relation to global gross domestic product (GDP)</ns1:description>
        <ns1:index>1.5.2</ns1:index>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/mdebe/ontologies/UN_SDG#NoPovertyIndicator1-5-3 -->

    <owl:NamedIndividual about="http://www.semanticweb.org/mdebe/ontologies/UN_SDG#NoPovertyIndicator1-5-3">
        <type resource="http://www.semanticweb.org/mdebe/ontologies/UN_SDG#Indicator"/>
        <ns1:description>Number of countries with national and local disaster risk reduction strategies</ns1:description>
        <ns1:index>1.5.3</ns1:index>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/mdebe/ontologies/UN_SDG#NoPovertyIndicator1-5-4 -->

    <owl:NamedIndividual about="http://www.semanticweb.org/mdebe/ontologies/UN_SDG#NoPovertyIndicator1-5-4">
        <type resource="http://www.semanticweb.org/mdebe/ontologies/UN_SDG#Indicator"/>
        <ns1:description>Proportion of local governments that adopt and implement local disaster risk reduction strategies in line with national disaster risk reduction strategies</ns1:description>
        <ns1:index>1.5.4</ns1:index>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/mdebe/ontologies/UN_SDG#NoPovertyIndicator1-a-1 -->

    <owl:NamedIndividual about="http://www.semanticweb.org/mdebe/ontologies/UN_SDG#NoPovertyIndicator1-a-1">
        <type resource="http://www.semanticweb.org/mdebe/ontologies/UN_SDG#Indicator"/>
        <ns1:description>Proportion of resources allocated by the government directly to poverty reduction programmes</ns1:description>
        <ns1:index>1.a.1</ns1:index>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/mdebe/ontologies/UN_SDG#NoPovertyIndicator1-a-2 -->

    <owl:NamedIndividual about="http://www.semanticweb.org/mdebe/ontologies/UN_SDG#NoPovertyIndicator1-a-2">
        <type resource="http://www.semanticweb.org/mdebe/ontologies/UN_SDG#Indicator"/>
        <ns1:description>Proportion of total government spending on essential services (education, health and social protection)</ns1:description>
        <ns1:index>1.a.2</ns1:index>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/mdebe/ontologies/UN_SDG#NoPovertyIndicator1-b-1 -->

    <owl:NamedIndividual about="http://www.semanticweb.org/mdebe/ontologies/UN_SDG#NoPovertyIndicator1-b-1">
        <type resource="http://www.semanticweb.org/mdebe/ontologies/UN_SDG#Indicator"/>
        <ns1:description>Proportion of government recurrent and capital spending to sectors that disproportionately benefit women, the poor and vulnerable groups</ns1:description>
        <ns1:index>1.b.1</ns1:index>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/mdebe/ontologies/UN_SDG#NoPovertyTarget1-1 -->

    <owl:NamedIndividual about="http://www.semanticweb.org/mdebe/ontologies/UN_SDG#NoPovertyTarget1-1">
        <type resource="http://www.semanticweb.org/mdebe/ontologies/UN_SDG#NoPoverty"/>
        <ns1:hasIndicator resource="http://www.semanticweb.org/mdebe/ontologies/UN_SDG#NoPovertyIndicator1-1-1"/>
        <ns1:description>By 2030, eradicate extreme poverty for all people everywhere, currently measured as people living on less than $1.25 a day</ns1:description>
        <ns1:index datatype="http://www.w3.org/2001/XMLSchema#decimal">1.1</ns1:index>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/mdebe/ontologies/UN_SDG#NoPovertyTarget1-2 -->

    <owl:NamedIndividual about="http://www.semanticweb.org/mdebe/ontologies/UN_SDG#NoPovertyTarget1-2">
        <type resource="http://www.semanticweb.org/mdebe/ontologies/UN_SDG#NoPoverty"/>
        <ns1:hasIndicator resource="http://www.semanticweb.org/mdebe/ontologies/UN_SDG#NoPovertyIndicator1-2-1"/>
        <ns1:hasIndicator resource="http://www.semanticweb.org/mdebe/ontologies/UN_SDG#NoPovertyIndicator1-2-2"/>
        <ns1:description>By 2030, reduce at least by half the proportion of men, women and children of all ages living in poverty in all its dimensions according to national definitions</ns1:description>
        <ns1:index datatype="http://www.w3.org/2001/XMLSchema#decimal">1.2</ns1:index>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/mdebe/ontologies/UN_SDG#NoPovertyTarget1-3 -->

    <owl:NamedIndividual about="http://www.semanticweb.org/mdebe/ontologies/UN_SDG#NoPovertyTarget1-3">
        <type resource="http://www.semanticweb.org/mdebe/ontologies/UN_SDG#NoPoverty"/>
        <ns1:hasIndicator resource="http://www.semanticweb.org/mdebe/ontologies/UN_SDG#NoPovertyIndicator1-3-1"/>
        <ns1:description>Implement nationally appropriate social protection systems and measures for all, including floors, and by 2030 achieve substantial coverage of the poor and the vulnerable</ns1:description>
        <ns1:index datatype="http://www.w3.org/2001/XMLSchema#decimal">1.3</ns1:index>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/mdebe/ontologies/UN_SDG#NoPovertyTarget1-4 -->

    <owl:NamedIndividual about="http://www.semanticweb.org/mdebe/ontologies/UN_SDG#NoPovertyTarget1-4">
        <type resource="http://www.semanticweb.org/mdebe/ontologies/UN_SDG#NoPoverty"/>
        <ns1:hasIndicator resource="http://www.semanticweb.org/mdebe/ontologies/UN_SDG#NoPovertyIndicator1-4-1"/>
        <ns1:hasIndicator resource="http://www.semanticweb.org/mdebe/ontologies/UN_SDG#NoPovertyIndicator1-4-2"/>
        <ns1:description>By 2030, ensure that all men and women, in particular the poor and the vulnerable, have equal rights to economic resources, as well as access to basic services, ownership and control over land and other forms of property, inheritance, natural resources, appropriate new technology and financial services, including microfinance</ns1:description>
        <ns1:index datatype="http://www.w3.org/2001/XMLSchema#decimal">1.4</ns1:index>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/mdebe/ontologies/UN_SDG#NoPovertyTarget1-5 -->

    <owl:NamedIndividual about="http://www.semanticweb.org/mdebe/ontologies/UN_SDG#NoPovertyTarget1-5">
        <type resource="http://www.semanticweb.org/mdebe/ontologies/UN_SDG#NoPoverty"/>
        <ns1:hasIndicator resource="http://www.semanticweb.org/mdebe/ontologies/UN_SDG#NoPovertyIndicator1-5-1"/>
        <ns1:hasIndicator resource="http://www.semanticweb.org/mdebe/ontologies/UN_SDG#NoPovertyIndicator1-5-2"/>
        <ns1:hasIndicator resource="http://www.semanticweb.org/mdebe/ontologies/UN_SDG#NoPovertyIndicator1-5-3"/>
        <ns1:hasIndicator resource="http://www.semanticweb.org/mdebe/ontologies/UN_SDG#NoPovertyIndicator1-5-4"/>
        <ns1:description>By 2030, build the resilience of the poor and those in vulnerable situations and reduce their exposure and vulnerability to climate-related extreme events and other economic, social and environmental shocks and disasters</ns1:description>
        <ns1:index datatype="http://www.w3.org/2001/XMLSchema#decimal">1.5</ns1:index>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/mdebe/ontologies/UN_SDG#NoPovertyTarget1-a -->

    <owl:NamedIndividual about="http://www.semanticweb.org/mdebe/ontologies/UN_SDG#NoPovertyTarget1-a">
        <type resource="http://www.semanticweb.org/mdebe/ontologies/UN_SDG#NoPoverty"/>
        <ns1:hasIndicator resource="http://www.semanticweb.org/mdebe/ontologies/UN_SDG#NoPovertyIndicator1-a-1"/>
        <ns1:hasIndicator resource="http://www.semanticweb.org/mdebe/ontologies/UN_SDG#NoPovertyIndicator1-a-2"/>
        <ns1:description>Ensure significant mobilization of resources from a variety of sources, including through enhanced development cooperation, in order to provide adequate and predictable means for developing countries, in particular least developed countries, to implement programmes and policies to end poverty in all its dimensions</ns1:description>
        <ns1:index>1.a</ns1:index>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/mdebe/ontologies/UN_SDG#NoPovertyTarget1-b -->

