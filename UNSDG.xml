    <rdf:type rdf:resource="http://www.semanticweb.org/mdebe/ontologies/UN_SDG#NoPoverty"/>
    <ns1:hasIndicator xmlns:ns1="http://www.semanticweb.org/mdebe/ontologies/UN_SDG#" rdf:resource="http://www.semanticweb.org/mdebe/ontologies/UN_SDG#NoPovertyIndicator1-a-1"/>
